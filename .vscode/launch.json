    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in library 'luckyapi'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=luckyapi"
                ],
                "filter": {
                    "name": "luckyapi",
                    "kind": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'luckyapi'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=luckyapi",
                    "--package=luckyapi"
                ],
                "filter": {
                    "name": "luckyapi",
                    "kind": "bin"
                }
            },
            "args": ["launch-x-server","--port","8080"],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'luckyapi'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=luckyapi",
                    "--package=luckyapi"
                ],
                "filter": {
                    "name": "luckyapi",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in library 'luckylib'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=luckylib"
                ],
                "filter": {
                    "name": "luckylib",
                    "kind": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}